#include<stdio.h>
int main()
{
    int n;
    printf("enter the size of the ARRAY");
    scanf("%d",&n);
    int array[n];
    printf("enter the array element:");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    int sum=0;
    for(int j=0;j<n;j++)
    {
        sum+=array[j];
    }
    printf("the sum of all the elements in the array is %d ",sum);
    return 0;
}
